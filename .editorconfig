root = true
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.rs]
indent_style = space
indent_size = 4

[*.js,*.ts]
indent_style = space
indent_size = 2

[help.stdout.txt]
indent_style = tab
tab_width = 4

[*.gitattributes]
indent_style = tab
tab_width = 24
